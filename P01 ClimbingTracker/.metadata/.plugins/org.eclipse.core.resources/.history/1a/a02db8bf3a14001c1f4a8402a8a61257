public static void main(String[] args) {
	final int LEVELS_CAPACITY = 5;
	String[] sendArray = new String[LEVELS_CAPACITY];
	int numSend = 0;
	
	//scenario 1 : empty array
	String grade= null;
	for(int i = 0; i < LEVELS_CAPACITY; i++) {
		sendArray[i] = grade;
	} 
	System.out.println(ClimbingTracker.sendClimb(sendArray, numSend, grade));
	
		
	}
	public static void printOversizeArray(String[ ]arrayRef, int arraySize) {
		int index;
		
		System.out.print("[");
		for(index=0; index < arraySize; ++index){
			System.out.print(arrayRef[index]);
			if(index != arraySize) {
				System.out.print(",");
			}
		}
		System.out.print("]  ");
		System.out.println("numSend: "+ totalNumSend);
	}
